    <script>
        var score = prompt('请输入你的成绩');
        if(score >= 90){
            alert('恭喜你获得了A');
        }else if(score >= 80 && score < 90){
            alert('恭喜你获得了B');
        }else if(score >= 70 && score < 80){
            alert('恭喜你获得了C');
        }else if(score >= 60 && score < 70){
            alert('恭喜你获得了D');
        }else{
            alert('不好意思，你获得了E');
        }
    </script>

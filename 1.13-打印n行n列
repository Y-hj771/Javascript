    <script>
        var i = prompt('请输入你要打印的行数');
        var j = prompt('请输入你要打印的列数');
        var str = '';
        for(var x = 0;x<i;x++){
            for(var y = 0;y<j;y++){
                str = str + '☆';
            }
            str = str + '\n';
        }
        alert(str);
    </script>
